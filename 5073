a, b, c = map(int, input().split())
while not a == b == c == 0:
    if list(sorted([a, b, c]))[2] >= list(sorted([a, b, c]))[1]+list(sorted([a, b, c]))[0]:
        print("Invalid")
    elif a== b == c:
        print("Equilateral")
    elif len({a, b, c}) == 2:
        print("Isosceles")
    elif len({a, b, c}) == 3:
        print("Scalene")
    a, b, c = map(int, input().split())