import sys
input = sys.stdin.readline

melon = int(input())
dx = [0, 1, -1, 0, 0]
dy = [0, 0, 0, -1, 1]
v = []
x, y = 0, 0
for _ in range(6):
    direction, length = map(int, input().split())
    x += dx[direction]*length
    y += dy[direction]*length
    v.append([x, y])
v.append([v[0][0], v[0][1]])

# ans = v[1][0]*v[0][1] - v[2][0]*v[1][1]
ans = 0
for i in range(6):
    ans += v[i][0]*v[i+1][1]
for i in range(1, 7):
    ans -= v[i][0]*v[i-1][1]

print(abs(ans*melon)//2)